{
  "name": "timeseries-service",
  "version": "1.0.2",
  "description": "Microservice responsible for time series on the HANIoT platform.",
  "main": "dist/server.js",
  "scripts": {
    "build": "gulp build",
    "build:doc": "typedoc --options typedoc.json",
    "start": "node dist/server.js",
    "start:dev": "gulp dev",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc --clean --reporter=text-summary mocha test/**/*.spec.ts",
    "test:unit": "nyc --clean --all --reporter=text-summary mocha test/unit/**/*.spec.ts",
    "test:integration": "nyc --clean --all --reporter=text-summary mocha test/integration/**/*.spec.ts",
    "test:cov": "nyc --clean --all --reporter=html --reporter=text mocha test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haniot/timeseries.git"
  },
  "bugs": {
    "url": "https://github.com/haniot/timeseries/issues"
  },
  "homepage": "https://github.com/haniot/timeseries#readme",
  "keywords": [
    "microservice",
    "timeseries",
    "intraday",
    "nodejs",
    "typescipt",
    "express"
  ],
  "author": "NUTES/UEPB",
  "license": "Apache-2.0",
  "nyc": {
    "include": [
      "**/*.tsx",
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*",
      "config/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "amqp-client-node": "^1.0.10",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^2.1.4",
    "influx": "^5.6.3",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^8.0.4",
    "@types/morgan": "^1.9.2",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
